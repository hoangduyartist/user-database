<div id="<%=typeof item!= 'undefined' ? item.name : ''%>" class="jumbotron py-4 hover-effect">
    <div class="container">
        <h1 class="display-4 text-center"><%=typeof item!= 'undefined' ? item.name : ''%></h1>
        <hr class="my-2">
        <div class="row">
            <div class="col-6">
                <p class="lead">End point: <strong>/<%=typeof item!= 'undefined' ? item.endpoint : ''%></strong></p>
            </div>
            <div class="col-6">
                <p class="lead text-right">Method: <strong><%=typeof item!= 'undefined' ? item.method : ''%></strong>
                </p>
            </div>
        </div>
        <p class="lead">url:
            <strong
                id="url<%= typeof key!='undefined' ? key : '' %>">http://142.93.253.93:81/web-api/<%=typeof item!= 'undefined' ? item.endpoint : ''%></strong>
            &nbsp;&nbsp;&nbsp;<smal><button id="copyUrlBtn<%= typeof key!='undefined' ? key : '' %>"
                    class="btn btn-sm btn-outline-primary p-1" onclick="copyToClipboard(`#url<%= typeof key!='undefined' ? key : '' %>`)">copy</button></smal>
        </p>
        <div>
            <label class="lead">Description:</label>
            <p class="lead d-inline"><%=typeof item!= 'undefined' ? item.description : '' %></p>
        </div>
        <label class="lead">Return:</label>
        <div class="lead">
            <ul>
                <% if(typeof item != 'undefined') { %>
                <% item.return.forEach((returnList, key)=>{ %>
                <li><%= returnList %></li>
                <% }) %>
                <% } %>
            </ul>
            <!-- <%=typeof item!= 'undefined' ? item.return : '' %> -->
        </div>
    </div>
</div>

<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
    integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
    crossorigin="anonymous"></script>
<script>

    function copyToClipboard(element) {
        var $temp = $("<input>");
        $("body").append($temp);
        $temp.val($(element).text()).select();
        document.execCommand("copy");
        $temp.remove();
    }

    // let copyText = document.getElementById("#url<%= typeof key!='undefined' ? key : '' %>");
    // copyText.textContent();
    // document.execCommand("copy");
    // alert("Copied the text: " + copyText.value);
    // copyText.remove();
</script>
